{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "plugins": [
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "semi": ["error", "never"],
        "func-names": "warn",
        "no-console": "off",
        "prettier/prettier": ["error", {"semi": false, "singleQuote": true}]
    }
}
